Resources:
  AWSEBAutoScalingGroup:
    Type: AWS::AutoScaling::AutoScalingGroup
    Properties:
      LaunchTemplate:
        LaunchTemplateSpecification:
          LaunchTemplateId: !Ref AWSEBLaunchTemplate
          Version: !GetAtt AWSEBLaunchTemplate.LatestVersionNumber
      MinSize: 1
      MaxSize: 4